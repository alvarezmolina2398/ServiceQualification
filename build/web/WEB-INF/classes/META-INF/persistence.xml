<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="ServiceQualificationPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>com.qualification.modelo.Interrogante</class>
    <class>com.qualification.modelo.Usuario</class>
    <class>com.qualification.modelo.Agencia</class>
    <class>com.qualification.modelo.Parametro</class>
    <class>com.qualification.modelo.Departamento</class>
    <class>com.qualification.modelo.Promocion</class>
    <class>com.qualification.modelo.Cooperativa</class>
    <class>com.qualification.modelo.Asociado</class>
    <exclude-unlisted-classes>false</exclude-unlisted-classes>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://10.10.10.17:3306/qualification?zeroDateTimeBehavior=convertToNull"/>
      <property name="javax.persistence.jdbc.user" value="user"/>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
      <property name="javax.persistence.jdbc.password" value="user"/>
      <property name="javax.persistence.schema-generation.database.action" value="create"/>
    </properties>
  </persistence-unit>
</persistence>
